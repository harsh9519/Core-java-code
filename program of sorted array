--------sorted method are use to arrange increasing order

package sortarray;

public class C {

	public static void main(String[] args) {
		
		int[] arr = {23,34,56,78,98,54,45,43,67};
		int temp=0,a=0,b=0;
		for(int i = 0;i<arr.length;i++)
		{
			for(int j = i+1;j<arr.length;j++)
			{
				if(arr[j]<arr[i])
				{
				temp = arr[j];
				arr[j] = arr[i];
				arr[i] = temp;
				}
			}
		}
		
		for(int arr0:arr)
		{
			System.out.println(arr0);
		}
		// TODO Auto-generated method stub

	}

}
